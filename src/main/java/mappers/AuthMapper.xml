<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="AuthMAP">


	<insert id="insertAuth" parameterType="auth">
		INSERT INTO auth
		(m_id, auth)
		VALUES
		(#{m_id},'ROLE_USER')
	</insert>
	

	
	

</mapper>